<div class="row">
    {{>adminMenu}}
    <div class="admin-posts">
        <h3>Статьи</h3>
        <table class="table">
            <thead>
                <tr>
                    <th>Название</th>
                    <th>Дата создания</th>
                    <th>Действия</th>
                </tr>
            </thead>
            <tbody>
                {{#each posts}}
                    <tr>
                        <td>{{title}}</td>
                        <td>{{formatDate createdAt}}</td>
                        <td><a href="#">Редактировать</a></td>
                    </tr>
                {{/each}}
            </tbody>
        </table>
    </div>
</div>
